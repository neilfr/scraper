<ul>
    {{#each articles}}
    <div class="articleContainer">
        <div class="article">
            <a href={{link}}>
                <p class="headline">{{headline}}</p>
            </a>
            <p class="lead">{{lead}}</p>
        </div>
        <button id={{_id}} class="notesBtn btn btn-primary btn-sm">Notes</button>
        <button id={{_id}} class="deleteBtn btn btn-danger btn-sm">Delete</button>
    </div>
    {{/each}}
</ul>